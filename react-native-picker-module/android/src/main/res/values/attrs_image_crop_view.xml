<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BloomNativeImageCropView">
        <attr name="bloom_native_gridInnerStroke" format="dimension" />
        <attr name="bloom_native_gridInnerColor" format="color" />
        <attr name="bloom_native_setInnerGridMode" format="integer">
            <enum name="off" value="0"/>
            <enum name="on" value="1"/>
        </attr>

        <attr name="bloom_native_gridOuterStroke" format="dimension" />
        <attr name="bloom_native_gridOuterColor" format="color" />
        <attr name="bloom_native_setOuterGridMode" format="integer">
            <enum name="off" value="0"/>
            <enum name="on" value="1"/>
        </attr>
        <attr name="bloom_native_gridLeftRightMargin" format="dimension" />
        <attr name="bloom_native_gridTopBottomMargin" format="dimension" />
        <attr name="bloom_native_outsideLayerColor" format="color" />
    </declare-styleable>
    
</resources>
